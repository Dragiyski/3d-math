    implementation[Vector<%= size -%>D_polymorphism].normalize = function normalize(x) {
        return implementation[Vector<%= size -%>D_polymorphism][Number_polymorphism].div(x, implementation[Vector<%= size -%>D_polymorphism].length(x));
